            <xs:sequence>
               
                <xs:element name="person" type="pType" minOccurs="0" maxOccurs="unbounded"></xs:element>
            </xs:sequence>
        </xs:complexType>
  
        </xs:element>
   
       <xs:complexType name="pType">
           <xs:sequence>
